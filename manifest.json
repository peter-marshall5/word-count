{
    "name": "Word Counter",
    "version": "1.0.0",
    "description": "Count how many words are on a page!",
    "manifest_version": 3,
    "author": "Peter Marshall",
    "action":{
	"default_popup": "index.html",
        "default_title": "Word Count"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
  "permissions": ["<all_urls>", "management"],
  "icons": {
	  "128": "icon.png"
  }
}
